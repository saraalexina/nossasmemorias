    <!-- typewriting js -->
     <script class = "lead">
      function atualizarContagem() {
        const inicio = new Date("2025-02-01T00:00:00");
        const agora = new Date();

        const diff = agora - inicio;

        const dias = Math.floor(diff / (1000 * 60 * 60 * 24));
        const horas = Math.floor((diff / (1000 * 60 * 60)) % 24);
        const minutos = Math.floor((diff / (1000 * 60)) % 60);
        const segundos = Math.floor((diff / 1000) % 60);

        const texto = `<span style="font-size: 3rem; font-weight: 300;  line-height: 1.8;">Me apaixonando h√°: <strong>${dias}</strong> dias, <strong>${horas}</strong> horas, <strong>${minutos}</strong> minutos e <strong>${segundos}</strong> segundos üíñ</span>`;

        
        document.getElementById("contador-dias").innerHTML = texto;
      }

      setInterval(atualizarContagem, 1000);
      atualizarContagem(); // executa logo ao carregar

        // Lista de mensagens
        const mensagens = [
        "Voc√™ √© a minha felicidade di√°ria! üòç",
        "Te amo mais do que a medida üôà‚ù§Ô∏è",
        "Que seu dia seja t√£o lindo quanto o seu sorriso üåôüå∏",
        "Meu mundo √© mais colorido com voc√™ üåà",
        ];

        // Fun√ß√£o para mostrar notifica√ß√£o estilizada
        function mostrarNotificacao(mensagem) {
        const notif = document.createElement("div");
        notif.classList.add("notificacao");
        notif.innerHTML = mensagem;

        document.body.appendChild(notif);

        // Mostrar com anima√ß√£o
        setTimeout(() => {
            notif.classList.add("show");
        }, 100);

        // Esconder e remover ap√≥s 5 segundos
        setTimeout(() => {
            notif.classList.remove("show");
            setTimeout(() => {
            notif.remove();
            }, 500);
        }, 5000);
        }

        // Ao abrir a p√°gina, mostra mensagem do dia
        window.onload = () => {
        const hoje = new Date().getDay(); // 0 = Domingo, 1 = Segunda...
        const mensagemHoje = mensagens[hoje % mensagens.length];
        mostrarNotificacao(mensagemHoje);
        };

    </script>
    <script src = "typewriting-master/typewriting.js"></script>
